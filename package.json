{
    "name": "geeking-admin",
    "type": "module",
    "version": "0.9.18",
    "private": true,
    "description": "a clean and concise back-end management template based on Vue3, Vite5, Typescript, and Naive UI.",
    "author": {
        "name": "geek-tim",
        "email": "841787507@qq.com",
        "url": "https://github.com/geek-tim/geeking-admin"
    },
    "license": "MIT",
    "repository": {
        "url": "https://github.com/geek-tim/geeking-admin.git"
    },
    "bugs": {
        "url": "https://github.com/geek-tim/geeking-admin/issues"
    },
    "keywords": [
        "Vue",
        "Vue3",
        "admin",
        "admin-template",
        "vue-admin",
        "vue-admin-template",
        "Vite5",
        "Vite",
        "vite-admin",
        "TypeScript",
        "TS",
        "NaiveUI",
        "naive-ui",
        "naive-admin",
        "NaiveUI-Admin",
        "naive-ui-admin",
        "UnoCSS"
    ],
    "scripts": {
        "dev": "vite --mode dev --port 9980",
        "dev:prod": "vite --mode production",
        "build": "vite build",
        "build:dev": "vite build --mode dev",
        "preview": "vite preview --port 9981",
        "sizecheck": "npx vite-bundle-visualizer",
        "test:e2e": "cypress open",
        "test:e2e:run": "cypress run",
        "test": "vitest",
        "test:coverage": "vitest --coverage",
        "prettier": "prettier --write .",
        "prepare": "husky",
        "lint": "eslint . --fix --cache",
        "commit:comment": "引导设置规范化的提交信息",
        "commit": "git-cz",
        "release": "standard-version"
    },
    "dependencies": {
        "@vueuse/core": "^13.6.0",
        "axios": "^1.13.2",
        "colord": "^2.9.3",
        "echarts": "^5.6.0",
        "md-editor-v3": "^5.6.1",
        "nprogress": "^0.2.0",
        "pinia": "^3.0.3",
        "pinia-plugin-persistedstate": "^4.4.1",
        "pro-naive-ui": "^2.4.3",
        "quill": "^2.0.3",
        "radash": "^12.1.1",
        "vue": "^3.5.18",
        "vue-draggable-plus": "^0.6.0",
        "vue-i18n": "^11.1.11",
        "vue-router": "^4.5.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^20.3.1",
        "@commitlint/config-conventional": "^20.3.1",
        "@iconify-json/icon-park-outline": "^1.2.2",
        "@iconify/vue": "^5.0.0",
        "@types/node": "^24.1.0",
        "@types/nprogress": "^0.2.3",
        "@vitejs/plugin-vue": "^6.0.1",
        "@vitejs/plugin-vue-jsx": "^5.0.1",
        "@vitest/coverage-v8": "4.0.17",
        "@vitest/eslint-plugin": "^1.6.4",
        "@vue/eslint-config-prettier": "^10.2.0",
        "@vue/eslint-config-typescript": "^14.6.0",
        "@vue/test-utils": "^2.4.6",
        "commitizen": "^4.3.1",
        "commitlint-config-cz": "^0.13.3",
        "cypress": "^15.8.2",
        "cz-conventional-changelog": "^3.3.0",
        "cz-customizable": "^7.5.1",
        "eslint": "^9.39.2",
        "eslint-plugin-cypress": "^5.2.0",
        "eslint-plugin-vue": "~10.6.2",
        "happy-dom": "^20.1.0",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.2",
        "msw": "^2.12.7",
        "naive-ui": "^2.42.0",
        "prettier": "^3.7.4",
        "sass": "^1.89.2",
        "simple-git-hooks": "^2.13.1",
        "standard-version": "^9.5.0",
        "typescript": "^5.8.3",
        "unocss": "^66.3.3",
        "unplugin-auto-import": "^19.3.0",
        "unplugin-icons": "^22.2.0",
        "unplugin-vue-components": "^28.8.0",
        "vite": "^7.0.6",
        "vite-bundle-visualizer": "^1.2.1",
        "vite-plugin-compression": "^0.5.1",
        "vite-plugin-vue-devtools": "8.0.0",
        "vitest": "^4.0.17",
        "vue-tsc": "^3.0.5"
    },
    "lint-staged": {
        "src/**/*.{ts,js,jsx,tsx,vue}": [
            "npm run lint",
            "npm run prettier"
        ]
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-customizable"
        },
        "cz-customizable": {
            "config": "./.cz-config.cjs"
        }
    },
    "msw": {
        "workerDirectory": [
            "public"
        ]
    }
}
